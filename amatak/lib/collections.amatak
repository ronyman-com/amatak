function map(arr, func) {
    let result = []
    for let i = 0; i < len(arr); i = i + 1 {
        result.push(func(arr[i]))
    }
    return result
}

function filter(arr, func) {
    let result = []
    for let i = 0; i < len(arr); i = i + 1 {
        if func(arr[i]) {
            result.push(arr[i])
        }
    }
    return result
}